{
	"name": "de-swebhosting-typo3-extension/news-richteaser",
	"description": "Allows TYPO3 content elments in news teasers.",
	"type": "typo3-cms-extension",
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Alexander Stehlik",
			"role": "Developer"
		}
	],
	"replace": {
		"news_richteaser": "self.version",
		"typo3-ter/news-richteaser": "self.version"
	},
	"require": {
		"typo3/cms-core": "^7.6",
		"georgringer/news": "^4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8",
		"codeception/codeception": "^2.1"
	},
	"autoload": {
		"psr-4": {
			"Int\\NewsRichteaser\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Int\\NewsRichteaser\\Tests\\": "Tests",
			"TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	},
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/"}
	]
}
